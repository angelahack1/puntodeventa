  },
  "dependencies": {
    "lightningcss": "^1.29.3",
    "mongodb": "^6.16.0",
    "next": "14.1.0",
    "nodemon": "^3.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.57",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
