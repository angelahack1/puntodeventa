{
  "name": "puntodeventa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev2": "nodemon",
    "build": "next build",
    "start": "npx serve@latest out -p 31000",
    "clean_windows": "(if exist node_modules rd /s /q node_modules) && (if exist .next rd /s /q .next) && (if exist out rd /s /q out) && (if exist package-lock.json del package-lock.json)",
